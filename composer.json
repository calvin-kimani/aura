{
    "name": "calvin-kimani/aura",
    "description": "An admin dashboard for configuring and customizing every facet of your Laravel application, from users to databases and beyond.",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CalvinKimani\\Aura\\": "src/"
        }
    },
    "authors": [
        {
            "name": "calvin kimani",
            "email": "calvinkimani475@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/calvin-kimani/aura"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "CalvinKimani\\Aura\\Providers\\AuraServiceProvider"
            ]
        }
    }
}
